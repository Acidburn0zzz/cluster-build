label alpine
    kernel /kernel-arm64
    initrd /rancheros-initrd-arm64.img.gz
    devicetreedir /dtbs
    append earlycon=uart8250,mmio32,0xff130000 coherent_pool=1M ethaddr=${ethaddr} eth1addr=${eth1addr} serial=${serial#} rdinit=/init rancher.defaults.hostname=ros-${serial#} rancheros.autologin=tty0 rancher.password=mypassword rancher.docker.engine=docker-17.05.0-ce rancher.state.dev=LABEL=RANCHER_STATE rancher.state.autoformat=[/dev/mmcblk0p1]
