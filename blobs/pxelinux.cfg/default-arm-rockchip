label alpine
    kernel /kernel-arm64
    initrd /alpine-initrd-arm64.img.gz
    fdt /dtbs/rockchip/rk3328-rock64.dtb
    append earlycon=uart8250,mmio32,0xff130000 coherent_pool=1M ethaddr=${ethaddr} eth1addr=${eth1addr} serial=${serial#} ip=dhcp root=/dev/nfs rw nfsroot=192.168.88.1:/srv/nfs/ubuntu-zesty,vers=3,rsize=32768,wsize=32768
