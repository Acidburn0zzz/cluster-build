label rancheros-initrd
    kernel /kernel-arm64
    initrd /rancheros-initrd-arm64.img.gz
    devicetreedir /dtbs
    append coherent_pool=1M ethaddr=${ethaddr} serial=${serial#} rdinit=/init rancher.defaults.hostname=ros-${serial#} rancheros.autologin=tty0 rancher.password=mypassword rancher.state.dev=LABEL=RANCHER_STATE rancher.state.autoformat=[/dev/sda] rancher.state.wait=true rancher.state.required=true
